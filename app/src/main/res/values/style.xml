<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
   <style name="push_style">
		<item name="android:hint">@string/style_hint</item>
		<item name="android:inputType">number</item>
		<item name="android:textSize">14sp</item>
	</style>
	<style name="push_alias">
		<item name="android:hint">@string/alias_hint</item>
		<item name="android:textSize">14sp</item>
	</style>
	<style name="push_tag">
		<item name="android:hint">@string/tag_hint</item>
		<item name="android:textSize">14sp</item>
	</style>

	<style name="style_title">
		<item name="android:textSize">16sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:ellipsize">none</item>
		<item name="android:maxLines">2</item>
		<item name="android:lines">2</item>
		<item name="android:textColor">@color/black</item>
	</style>
	<style name="style_content">
		<item name="android:textSize">14sp</item>
		<item name="android:textStyle">normal</item>
		<item name="android:ellipsize">end</item>
		<item name="android:maxLines">5</item>
		<item name="android:textColor">@color/black_444</item>
	</style>
	<style name="style_content_price">
		<item name="android:textSize">14sp</item>
		<item name="android:textStyle">normal</item>
		<item name="android:singleLine">true</item>
	</style>

	<style name="style_note_title">
		<item name="android:textSize">18sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:ellipsize">none</item>
		<item name="android:maxLength">32</item>
		<item name="android:textColor">@color/black</item>
	</style>
	<style name="style_note_content">
		<item name="android:textSize">16sp</item>
		<item name="android:textStyle">normal</item>
		<item name="android:ellipsize">end</item>
		<item name="android:textColor">@color/black_444</item>
	</style>
</resources>